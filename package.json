{
  "name": "jep-cigar-business-system",
  "version": "1.0.0",
  "description": "Premium digital tools for cigar entrepreneurs - modular business system",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "firebase:deploy": "firebase deploy --project cigar-56871",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules --project cigar-56871",
    "firebase:deploy:open": "firebase deploy --only firestore:rules --project cigar-56871",
    "firebase:deploy:indexes": "firebase deploy --only firestore:indexes --project cigar-56871",
    "firebase:deploy:hosting": "firebase deploy --only hosting --project cigar-56871",
    "fix:firestore": "node scripts/quick-fix-firestore.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.14.2",
    "antd": "^5.12.8",
    "chart.js": "^4.4.1",
    "date-fns": "^3.0.6",
    "dayjs": "^1.11.18",
    "firebase": "^10.7.1",
    "framer-motion": "^10.16.16",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-pdf": "^7.6.0",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.20.1",
    "react-spring": "^9.7.3",
    "react-window": "^2.1.1",
    "react-window-infinite-loader": "^2.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/lodash": "^4.14.202",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
